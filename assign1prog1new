void display(struct node *p)
{
  while(p!=NULL)
    {
      printf("%d", p->data);
      p = p->next;
    }
}

void createLinkList(char A[])
{
  int i=strlen(A) - 1;
  while(i>=0)
    {
      printf("%d \n", A[i]);
      i--;
    }

}


int main()
{
  char A[50]="";
  scanf("%s", A);
  
  // printf("%d\n", i);
  // int j = i-1;
  // printf("%d", i);
  // printf("%c", A[i-1]);
  // puts(A);
  createLinkList(A);
  // int A[] = {1,2,3,4,5};
  // create(A,5);
