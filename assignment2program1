#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>

struct date
{
  int day, month, year;
  // char day[2];
  // char month[3];
  // char year[5];
  char name[100];
};

char* splitString(char str[], int start, int len)
{
  int i,j=0;
  char *data = malloc(sizeof(char) * (len+1));
  char *ptr = str;
 
  for(i=0; i<len; i++)
    {
      data[i] = str[i+start];
    }
  data[i] = '\n';
  return data;
}


